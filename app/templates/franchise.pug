//- title: Index Page
extends layouts/_layout
block head
  - var title = 'Франшиза'
  - var headerTransparent = true
  - var activeMenu = 4
  - var headerEmpty = true
  - var footerEmpty = true
  - var franchise = true
  - var franchiseFooterNav = true;
block content
    // start franchise-promo
    .franchise-promo 
      .swiper-container.franchise-promo-slider
        .swiper-wrapper
          // start swiper-slide
          .swiper-slide
            .bg(style="background-image: url(img/franchise-bg1.jpg)")
            .slide-content
              .slide-top
                .rect-first
                  .rect-line.line1
                  .rect-line.line2
                  .rect-line.line3
                  .rect-line.line4
                  .rect-caption Франшиза
              .slide-middle
                h1.slide-heading Баланс жизни и деловой активности
                p Ваша независимость, ваша компания, ваше будущее
              .slide-bottom
                .rect-second
                  a.button.fancybox(href="#double-pp") Узнать больше
                  .rect-line.line1
                  .rect-line.line2
                  .rect-line.line3
                  .rect-line.line4
          // end swiper-slide
          // start swiper-slide
          .swiper-slide
            .bg(style="background-image: url(img/franchise-bg2.jpg)")
            .slide-content
              .slide-top
                .rect-first
                  .rect-line.line1
                  .rect-line.line2
                  .rect-line.line3
                  .rect-line.line4
                  .rect-caption Франшиза
              .slide-middle
                h1.slide-heading Легкий старт
                p Франшиза без паушального взноса
              .slide-bottom
                .rect-second
                  a.button.fancybox(href="#double-pp") Узнать больше
                  .rect-line.line1
                  .rect-line.line2
                  .rect-line.line3
                  .rect-line.line4
          // end swiper-slide
          // start swiper-slide
          .swiper-slide
            .bg(style="background-image: url(img/franchise-bg3.jpg)")
            .slide-content
              .slide-top
                .rect-first
                  .rect-line.line1
                  .rect-line.line2
                  .rect-line.line3
                  .rect-line.line4
                  .rect-caption Франшиза
              .slide-middle
                h1.slide-heading Комфорт и эффективность #[br] в управлении
                p Комплексное бизнес-сопровождение, штат профессиональных управляющих
              .slide-bottom
                .rect-second
                  a.button.fancybox(href="#double-pp") Узнать больше
                  .rect-line.line1
                  .rect-line.line2
                  .rect-line.line3
                  .rect-line.line4
          // end swiper-slide
        .swiper-pagination
    // end franchise-promo
    // start franchise-unique
    .franchise-unique
      h2.section-heading Уникальность бизнесс-концепции
      .container
        .row
          .franchise-unique-circle(data-aos="fade-up" data-aos-duration="1000" data-aos-anchor=".franchise-unique")
            svg.svg-circle(width="408px" height="408px" viewBox="0 0 412 412")
              circle(cx="206" cy="206" r="204" stroke="#ccdb2a" stroke-width="2")
            .inner
              .icon-mark-map
              h3.lined NewView
              p Персонификация бизнеса #[br] и лабораторного сервиса
          .col-xs-12.col-sm-6.col-md-4.col-lg-4(data-aos="fade-right" data-aos-duration="1000" data-aos-delay="600" data-aos-anchor=".franchise-unique")
            .franchise-unique-item.lined
              .circle
                span 1
              h5 Бренд
              p персонифицированной диагностики #[br] и премиальных стандартов обслуживания
          .col-xs-12.col-sm-6.col-md-4.col-lg-4.col-md-offset-4.col-lg-offset-4(data-aos="fade-left" data-aos-duration="1000" data-aos-delay="1200" data-aos-anchor=".franchise-unique")
            .franchise-unique-item.lined
              .circle
                span 2
              h5 Новый подход
              p к сознанию потребителя: #[br] человек — менеджер своего здоровья
          .col-xs-12.col-sm-6.col-md-4.col-lg-4(data-aos="fade-right" data-aos-duration="1000" data-aos-delay="1800" data-aos-anchor=".franchise-unique")
            .franchise-unique-item
              .circle
                span 3
              h5 Идеальный бизнес-климат —
              p партнерские отношения в диалоге
          .col-xs-12.col-sm-6.col-md-4.col-lg-4.col-md-offset-4.col-lg-offset-4(data-aos="fade-left" data-aos-duration="1000" data-aos-delay="2400" data-aos-anchor=".franchise-unique")
            .franchise-unique-item
              .circle
                span 4
              h5 Собственный медицинский бизнес — 
              p часть общей системы здравоохранения России, ваш вклад в здоровье нации.
    // end franchise-unique
    // start d3_tur
    .d3_tur.d3_tur-secondary
      .container
        .desc_txt
          p Концепт медицинского офиса с индивидуальным дизайном под помещение партнера
        .d3c
          .crc
            p.tur_txt the future
            p.lab lab
            .of of
            .new_txt
              .n
                img(src='img/n.png', alt='')
              .e
                img(src='img/e.png', alt='')
              .w
                img(src='img/w.png', alt='')
            .play
        .crc_lines
          .crc_ln0
          .crc_ln1
          .crc_ln2
    // end d3_tur
    // start formats
    section.formats 
      .container
        h2.section-heading Мультиформатность франчайзингового предложения
        .formats-group
          .row
            .col-xs-12.col-sm-4.col-md-4.col-lg-4(data-aos="fade-in" data-duration="1000" data-aos-delay="300" data-aos-anchor=".formats")
              .formats-item
                  .circle(data-aos="zoom-out" data-aos-duration="1000" data-aos-anchor=".formats" data-aos-delay="300")
                    .icon.icon-check
                  .inner
                    h4 Premium #[span формат]
            .col-xs-12.col-sm-4.col-md-4.col-lg-4.col-lined(data-aos="fade-in" data-duration="1000" data-aos-delay="2000" data-aos-anchor=".formats")
              .formats-item.formats-item-arrowed
                .circle(data-aos="zoom-out" data-aos-duration="1000" data-aos-anchor=".formats" data-aos-delay="2000")
                  .icon.icon-check
                .inner
                  h4 Business #[span формат]  
            .col-xs-12.col-sm-4.col-md-4.col-lg-4(data-aos="fade-in" data-duration="1000" data-aos-delay="1100" data-aos-anchor=".formats")
              .formats-item
                .circle(data-aos="zoom-out" data-aos-duration="1000" data-aos-anchor=".formats" data-aos-delay="1100")
                  .icon.icon-check
                .inner
                  h4 Privilege #[span формат]
      .invest-price
        .container
          .invest-price-group
            .col-xs-12.col-sm-4.col-md-4.col-lg-4.nopad
              .invest-price-item
                h3 0,00
                p паушальный взнос
            .col-xs-12.col-sm-4.col-md-4.col-lg-4.nopad
              .invest-price-item
                h3 50%
                p доходная часть
            .col-xs-12.col-sm-4.col-md-4.col-lg-4.nopad
              .invest-price-item
                h3 0%
                p роялти до 3 года работы
        .invest-success
          h3.lined Правильные инвестиции
          p Бизнес на рынке лабораторной диагностики стабильный и доходный. #[br] Нам важно, чтобы в бизнес пришли партнеры, которые ценят высокие стандарты качества лабораторных услуг и сервиса и разделяют наше видение
          a.button.button-icon.button-gray.fancybox(href="#choose-franchise")
            span Выбрать франшизу
            span.ic
              include data/list.svg
    // end formats
    // start franchise-steps
    section.franchise-steps.swiper-container
      h2.section-heading Сделайте первый шаг
      .franchise-group.swiper-wrapper
        // start franchise-step
        .franchise-step.swiper-slide.step1(style="background-image: url(img/step-bg1.jpg)")
          .inner
            .franchise-step-top
              .top-wrap
                .num.num-1 1
                .content Откройте для себя #[br] LabQuest
            .franchise-step-content
              p На ней претенденту может быть предоставлена финансовая информация, идет более конкретный разговор о перспективах и условиях сотрудничества. Для получения доступа к этим сведениям, у вас могут попросить подписать соглашение о неразглашении
            .franchise-step-bottom
              a.button.button-white(href="#") Узнать все этапы
        // end franchise-step
        // start franchise-step
        .franchise-step.swiper-slide.step2(style="background-image: url(img/step-bg2.jpg)")
          .inner
            .franchise-step-top
              .top-wrap
                .num 2
                .content Заполните #[br] анкету
            .franchise-step-content
              p На ней претенденту может быть предоставлена финансовая информация, идет более конкретный разговор о перспективах и условиях сотрудничества. Для получения доступа к этим сведениям, у вас могут попросить подписать соглашение о неразглашении
            .franchise-step-bottom
              a.button.button-white(href="#") Узнать все этапы
        // end franchise-step
        // start franchise-step
        .franchise-step.swiper-slide.step3(style="background-image: url(img/step-bg3.jpg)")
          .inner
            .franchise-step-top
              .top-wrap
                .num 3
                .content Задайте вопросы #[br] менеджеру франчайзера
            .franchise-step-content
              p На ней претенденту может быть предоставлена финансовая информация, идет более конкретный разговор о перспективах и условиях сотрудничества. Для получения доступа к этим сведениям, у вас могут попросить подписать соглашение о неразглашении
            .franchise-step-bottom
              a.button.button-white(href="#") Узнать все этапы
        // end franchise-step
        // start franchise-step
        .franchise-step.swiper-slide.step4(style="background-image: url(img/step-bg4.jpg)")
          .inner
            .franchise-step-top
              .top-wrap
                .num 4
                .content Первая встреча #[br] с франчайзером
            .franchise-step-content
              p На ней претенденту может быть предоставлена финансовая информация, идет более конкретный разговор о перспективах и условиях сотрудничества. Для получения доступа к этим сведениям, у вас могут попросить подписать соглашение о неразглашении
            .franchise-step-bottom
              a.button.button-white(href="#") Узнать все этапы
        // end franchise-step
        // start franchise-step
        .franchise-step.swiper-slide.step5(style="background-image: url(img/step-bg5.jpg)")
          .inner
            .franchise-step-top
              .top-wrap
                .num 5
                .content Посетите  лабораторию #[br] и флагманский офис
            .franchise-step-content
              p На ней претенденту может быть предоставлена финансовая информация, идет более конкретный разговор о перспективах и условиях сотрудничества. Для получения доступа к этим сведениям, у вас могут попросить подписать соглашение о неразглашении
            .franchise-step-bottom
              a.button.button-white(href="#") Узнать все этапы
        // end franchise-step
      .franchise-steps-bottom
        a.button.button-gray(href="#") Узнать все этапы
    // end franchise-steps
    // start franchise-priveleges
    section.franchise-priveleges 
      .container
        h2.section-heading Привилегии для инвесторов
        .group
          .row
            .col-xs-12.col-sm-6.col-md-6.col-lg-4
              .franchise-privelege
                .circle(data-aos="fade-up")
                  .icon.icon-check-large
                h4.lined Отсутствие #[br] конкурентов
                p Франшиза LabQuest — единственная медицинская франшиза в сфере персонифицированной диагностики и премиальных стандартов лабораторного сервиса
            .col-xs-12.col-sm-6.col-md-6.col-lg-4
              .franchise-privelege
                .circle(data-aos="fade-up")
                  .icon.icon-check-large
                h4.lined Единый центр #[br] снабжения 
                p LabQuest использует систему централизованного снабжения расходными материалами, элементами оформления медицинского офиса, обеспечения франчайзи полиграфической и рекламной продукцией и т. д.
            .col-xs-12.col-sm-6.col-md-6.col-lg-4
              .franchise-privelege
                .circle(data-aos="fade-up")
                  .icon.icon-check-large
                h4.lined Несколько потоков #[br] доходов франчайзи
                ul.custom-list
                  li 
                    span продажа лабораторных исследований
                  li 
                    span процедура взятия биоматериала
                  li 
                    span дополнительные мед. услуги и др.
            .col-xs-12.col-sm-6.col-md-6.col-lg-4
              .franchise-privelege
                .circle(data-aos="fade-up")
                  .icon.icon-check-large
                h4.lined Минимальная нагрузка #[br] на предпринимателя
                p LabQuest предлагает комплекс решений в зоне единой логистики, финансов, маркетинга, персонала и в других областях бизнеса
            .col-xs-12.col-sm-6.col-md-6.col-lg-4
              .franchise-privelege
                .circle(data-aos="fade-up")
                  .icon.icon-check-large
                h4.lined Преемственность #[br] бизнеса
                p Долгосрочный успех. Бизнес для обеспечения благополучной жизни близких. Бизнес, которым можно гордиться
            .col-xs-12.col-sm-6.col-md-6.col-lg-4
              .franchise-privelege
                .circle(data-aos="fade-up")
                  .icon.icon-check-large
                h4.lined Ответственность и #[br] открытость франчайзера
                p Регулярные бизнес-встречи менеджмента управляющей компании и партнеров-франчайзи
    // end franchise-priveleges
    // start business-features
    section.business-features 
      h2.section-heading Возможности для бизнеса
      .container
        .row
          .col-xs-12.col-sm-12.col-md-6.col-lg-6
            .business-feature
              h3.lined Монотерриториальные франчайзи
              p Приобретение одной франшизы. #[br] Развитие бренда локально
          .col-xs-12.col-sm-12.col-md-6.col-lg-6
            .business-feature
              h3.lined Политерриториальные франчайзи
              p Приобретение нескольких франшиз (постепенно) для ведения бизнеса и управления всеми франчайзинговыми медицинскими офисами самостоятельно или через наемного менеджера
          .col-xs-12.col-sm-12.col-md-6.col-lg-6
            .business-feature
              h3.lined Мастер-франшиза
              p Для тех, кто заинтересован в развитии #[br] бренда на широком уровне.
              p Региональная и межрегиональная экспансия
          .col-xs-12.col-sm-12.col-md-6.col-lg-6
            .business-feature
              h3.lined Продуманная реализация проекта
              p Франшиза LabQuest — ответ на каждый вопрос на  любом этапе развития бизнеса. Задача команды — снять с партнера бремя повседневных решений и сделать все, чтобы бизнес был простым и комфортным.
    // end business-features
    include partials/_popups.pug
