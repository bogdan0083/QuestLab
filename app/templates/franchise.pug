//- title: Index Page
extends layouts/_layout
block head
  - var title = 'Main page'
  - var headerTransparent = true
  - var activeMenu = 4
  - var headerEmpty = true
  - var footerEmpty = true
block content
    // start franchise-promo
    .franchise-promo 
      .swiper-container.franchise-promo-slider
        .swiper-wrapper
          // start swiper-slide
          .swiper-slide
            .bg(style="background-image: url(img/franchise-bg1.jpg)")
            .slide-content
              .slide-top
                .rect-first
                  .rect-line.line1
                  .rect-line.line2
                  .rect-line.line3
                  .rect-line.line4
                  .rect-caption Франшиза
              .slide-middle
                h1.slide-heading Баланс жизни и деловой активности
                p Ваша независимость, ваша компания, ваше будущее
              .slide-bottom
                .rect-second
                  a.button(href="#") Узнать больше
                  .rect-line.line1
                  .rect-line.line2
                  .rect-line.line3
                  .rect-line.line4
          // end swiper-slide
          // start swiper-slide
          .swiper-slide
            .bg(style="background-image: url(img/franchise-bg1.jpg)")
            .slide-content
              .slide-top
                .rect-first
                  .rect-line.line1
                  .rect-line.line2
                  .rect-line.line3
                  .rect-line.line4
                  .rect-caption Франшиза
              .slide-middle
                h1.slide-heading Легкий старт
                p Франшиза без паушального взноса и роялти
              .slide-bottom
                .rect-second
                  a.button(href="#") Узнать больше
                  .rect-line.line1
                  .rect-line.line2
                  .rect-line.line3
                  .rect-line.line4
          // end swiper-slide
          // start swiper-slide
          .swiper-slide
            .bg(style="background-image: url(img/franchise-bg1.jpg)")
            .slide-content
              .slide-top
                .rect-first
                  .rect-line.line1
                  .rect-line.line2
                  .rect-line.line3
                  .rect-line.line4
                  .rect-caption Франшиза
              .slide-middle
                h1.slide-heading Комфорт и эффективность #[br] в управлении
                p Комплексное бизнес-сопровождение, штат профессиональных управляющих
              .slide-bottom
                .rect-second
                  a.button(href="#") Узнать больше
                  .rect-line.line1
                  .rect-line.line2
                  .rect-line.line3
                  .rect-line.line4
          // end swiper-slide
        .swiper-pagination
    // end franchise-promo
    // start franchise-unique
    .franchise-unique
      h2.section-heading Уникальность бизнесс-концепции
      .container
        .row
          .franchise-unique-circle(data-aos="fade-up" data-aos-duration="1000" data-aos-anchor=".franchise-unique")
            .inner
              .icon-mark-map
              h3.lined Революционная
              p концепция для бизнеса #[br] и пациентов
          .col-xs-12.col-sm-6.col-md-4.col-lg-4(data-aos="fade-right" data-aos-duration="1000" data-aos-delay="600" data-aos-anchor=".franchise-unique")
            .franchise-unique-item.lined
              .circle
                span 1
              h5 Модель франшизы LabQuest 
              p не является типичной для рынка лабораторной диагностики
          .col-xs-12.col-sm-6.col-md-4.col-lg-4.col-md-offset-4.col-lg-offset-4(data-aos="fade-left" data-aos-duration="1000" data-aos-delay="1200" data-aos-anchor=".franchise-unique")
            .franchise-unique-item.lined
              .circle
                span 2
              h5 Франшиза LabQuest —
              p это проект со стабильной доходностью и широким набором услуг
          .col-xs-12.col-sm-6.col-md-4.col-lg-4(data-aos="fade-right" data-aos-duration="1000" data-aos-delay="1800" data-aos-anchor=".franchise-unique")
            .franchise-unique-item
              .circle
                span 3
              h5 Мы единственные в России,
              p кто создает розничную сеть медицинских офисов по лабораторной диагностике Premium формата
          .col-xs-12.col-sm-6.col-md-4.col-lg-4.col-md-offset-4.col-lg-offset-4(data-aos="fade-left" data-aos-duration="1000" data-aos-delay="2400" data-aos-anchor=".franchise-unique")
            .franchise-unique-item
              .circle
                span 4
              h5 Франшиза объединяет
              p
                | бренд LabQuest и суббренды Doctor Q,
                br 
                | LabQuest microbiology и LabQuest
                br 
                | histology
    // end franchise-unique
    // start d3_tur
    .d3_tur.d3_tur-secondary
      .container
        .desc_txt
          p Многопрофильный производственный комплекс LabQuest открыт!
        .d3c
          .crc
            p.tur_txt the future
            p.lab lab
            .of of
            .new_txt
              .n
                img(src='img/n.png', alt='')
              .e
                img(src='img/e.png', alt='')
              .w
                img(src='img/w.png', alt='')
            .play
        .crc_lines
          .crc_ln0
          .crc_ln1
          .crc_ln2
    // end d3_tur
    // start formats
    section.formats 
      .container
        h2.section-heading Мультиформатность франчайзингового предложения
        .formats-group
          .row
            .col-xs-12.col-sm-4.col-md-4.col-lg-4(data-aos="fade-in" data-duration="1000" data-aos-delay="300" data-aos-anchor=".formats")
              .formats-item
                  .circle(data-aos="zoom-out" data-aos-duration="1000" data-aos-anchor=".formats" data-aos-delay="300")
                    .icon.icon-check
                  .inner
                    h4 Премиум #[span формат]
            .col-xs-12.col-sm-4.col-md-4.col-lg-4.col-lined(data-aos="fade-in" data-duration="1000" data-aos-delay="2000" data-aos-anchor=".formats")
              .formats-item.formats-item-arrowed
                .circle(data-aos="zoom-out" data-aos-duration="1000" data-aos-anchor=".formats" data-aos-delay="2000")
                  .icon.icon-check
                .inner
                  h4 Франчайзинг #[span без вложения]  
            .col-xs-12.col-sm-4.col-md-4.col-lg-4(data-aos="fade-in" data-duration="1000" data-aos-delay="1100" data-aos-anchor=".formats")
              .formats-item
                .circle(data-aos="zoom-out" data-aos-duration="1000" data-aos-anchor=".formats" data-aos-delay="1100")
                  .icon.icon-check
                .inner
                  h4 Бизнес #[span формат]
      .invest-price
        .container
          .invest-price-group
            .col-xs-12.col-sm-4.col-md-4.col-lg-4.nopad
              .invest-price-item
                h3 0,00
                p паушальный взнос
            .col-xs-12.col-sm-4.col-md-4.col-lg-4.nopad
              .invest-price-item
                h3 0,00
                p роялти
            .col-xs-12.col-sm-4.col-md-4.col-lg-4.nopad
              .invest-price-item
                h3 Фикс. плата 
                p за ключевые экспертизы 
        .invest-success
          h3.lined Правильные инвестиции
          p Бизнес на рынке лабораторной диагностики стабильный и доходный. #[br] Нам важно, чтобы в бизнес пришли партнеры, которые ценят высокие стандарты качества лабораторных услуг и сервиса и разделяют наше видение
          a.button.button-icon.button-gray(href="#")
            span Выбрать франшизу
            span.ic
              include data/list.svg
    // end formats
    // start franchise-steps
    section.franchise-steps
      h2.section-heading Сделайте первый шаг
      .franchise-group
        // start franchise-step
        .franchise-step.step1(style="background-image: url(img/step-bg1.jpg)")
          .inner
            .franchise-step-top
              .top-wrap
                .num.num-1 1
                .content Откройте для себя #[br] LabQuest
            .franchise-step-content
              p На ней претенденту может быть предоставлена финансовая информация, идет более конкретный разговор о перспективах и условиях сотрудничества. Для получения доступа к этим сведениям, у вас могут попросить подписать соглашение о неразглашении
            .franchise-step-bottom
              a.button.button-white(href="#") Узнать все этапы
        // end franchise-step
        // start franchise-step
        .franchise-step.step2(style="background-image: url(img/step-bg2.jpg)")
          .inner
            .franchise-step-top
              .top-wrap
                .num 2
                .content Заполните #[br] анкету
            .franchise-step-content
              p На ней претенденту может быть предоставлена финансовая информация, идет более конкретный разговор о перспективах и условиях сотрудничества. Для получения доступа к этим сведениям, у вас могут попросить подписать соглашение о неразглашении
            .franchise-step-bottom
              a.button.button-white(href="#") Узнать все этапы
        // end franchise-step
        // start franchise-step
        .franchise-step.step3(style="background-image: url(img/step-bg3.jpg)")
          .inner
            .franchise-step-top
              .top-wrap
                .num 3
                .content Задайте вопросы #[br] менеджеру франчайзера
            .franchise-step-content
              p На ней претенденту может быть предоставлена финансовая информация, идет более конкретный разговор о перспективах и условиях сотрудничества. Для получения доступа к этим сведениям, у вас могут попросить подписать соглашение о неразглашении
            .franchise-step-bottom
              a.button.button-white(href="#") Узнать все этапы
        // end franchise-step
        // start franchise-step
        .franchise-step.step4(style="background-image: url(img/step-bg4.jpg)")
          .inner
            .franchise-step-top
              .top-wrap
                .num 4
                .content Первая встреча #[br] с франчайзером
            .franchise-step-content
              p На ней претенденту может быть предоставлена финансовая информация, идет более конкретный разговор о перспективах и условиях сотрудничества. Для получения доступа к этим сведениям, у вас могут попросить подписать соглашение о неразглашении
            .franchise-step-bottom
              a.button.button-white(href="#") Узнать все этапы
        // end franchise-step
        // start franchise-step
        .franchise-step.step5(style="background-image: url(img/step-bg5.jpg)")
          .inner
            .franchise-step-top
              .top-wrap
                .num 5
                .content Посетите  лабораторию #[br] и флагманский офис
            .franchise-step-content
              p На ней претенденту может быть предоставлена финансовая информация, идет более конкретный разговор о перспективах и условиях сотрудничества. Для получения доступа к этим сведениям, у вас могут попросить подписать соглашение о неразглашении
            .franchise-step-bottom
              a.button.button-white(href="#") Узнать все этапы
        // end franchise-step
    // end franchise-steps
    // start franchise-priveleges
    section.franchise-priveleges 
      .container
        h2.section-heading Привилегии для инвесторов
        .group
          .row
            .col-xs-12.col-sm-6.col-md-6.col-lg-4
              .franchise-privelege
                .circle(data-aos="fade-up")
                  .icon.icon-check-large
                h4.lined Эксклюзивный #[br] географический статус
                p LabQuest только выходит на рынок диагностических услуг. Инвесторы не ограничены в выборе территории для ведения бизнеса
            .col-xs-12.col-sm-6.col-md-6.col-lg-4
              .franchise-privelege
                .circle(data-aos="fade-up")
                  .icon.icon-check-large
                h4.lined Единый центр #[br] снабжения 
                p LabQuest использует систему централизованного снабжения расходными материалами, элементами оформления медицинского офиса обеспечения франчайзи полиграфической и рекламной продукцией и т. д.
            .col-xs-12.col-sm-6.col-md-6.col-lg-4
              .franchise-privelege
                .circle(data-aos="fade-up")
                  .icon.icon-check-large
                h4.lined Несколько потоков #[br] доходов франчайзи
                ul.custom-list
                  li 
                    span продажа лабораторных исследований
                  li 
                    span процедура взятия биоматериала
                  li 
                    span дополнительные мед. услуги и др.
            .col-xs-12.col-sm-6.col-md-6.col-lg-4
              .franchise-privelege
                .circle(data-aos="fade-up")
                  .icon.icon-check-large
                h4.lined Новые возможности на рынке лабораторной диагностики
                p Бизнес-идеи, основанные на особенностях национальных культур и религий 
            .col-xs-12.col-sm-6.col-md-6.col-lg-4
              .franchise-privelege
                .circle(data-aos="fade-up")
                  .icon.icon-check-large
                h4.lined Преемственность #[br] бизнеса
                p Долгосрочный успех. Бизнес для обеспечения благополучной жизни близких. Бизнес, которым можно гордиться
            .col-xs-12.col-sm-6.col-md-6.col-lg-4
              .franchise-privelege
                .circle(data-aos="fade-up")
                  .icon.icon-check-large
                h4.lined Неограниченный #[br] финансовый потенциал
                p Партнеры компании строят бизнес в процветающей отрасли
    // end franchise-priveleges
    // start business-features
    section.business-features 
      h2.section-heading Привилегии для инвесторов
      .container
        .row
          .col-xs-12.col-sm-12.col-md-6.col-lg-6
            .business-feature
              h3.lined Монотерриториальные франчайзи
              p Приобретение одной франшизы. #[br] Развитие бренда локально
          .col-xs-12.col-sm-12.col-md-6.col-lg-6
            .business-feature
              h3.lined Политерриториальные франчайзи
              p Приобретение нескольких франшиз (постепенно) для ведения бизнеса и управления всеми франчайзинговыми медицинскими офисами самостоятельно или через наемного менеджера
          .col-xs-12.col-sm-12.col-md-6.col-lg-6
            .business-feature
              h3.lined Дивизиональный менеджмент
              p Мастер-франшиза на страну/регион. #[br] Для тех, кто заинтересован в развитии бренда на широком уровне. Региональная и межрегиональная экспансия. 
              p Самостоятельное привлечение франчайзи, предоставление им постоянной поддержки и обучения при содействии со стороны франчайзера LabQuest
          .col-xs-12.col-sm-12.col-md-6.col-lg-6
            .business-feature
              h3.lined Политерриториальные франчайзи
              p Франшиза предоставляется только одному франчайзи с перспективами развития конкретной территории и с определенным количеством франчайзинговых точек (для исключения возможности работы на данной территории другого франчайзи)
    // end business-features
    include partials/_popups.pug
